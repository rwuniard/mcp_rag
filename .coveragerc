# Coverage configuration for MCP RAG project
[run]
source = src
omit = 
    */tests/*
    */test_*
    */__pycache__/*
    */venv/*
    */.venv/*

[report]
# Fail under 85%
fail_under = 85
show_missing = true
skip_covered = false
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov
title = MCP RAG Coverage Report
show_contexts = true

# XML and JSON outputs are disabled by default
# Uncomment these sections if needed:
# [xml]
# output = coverage.xml
# 
# [json]  
# output = coverage.json